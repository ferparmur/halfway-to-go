  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@data/*": ["./src/data/*"],
      "@img/*": ["./src/img/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@svg/*": ["./src/svg/*"]
    }
  }
}
